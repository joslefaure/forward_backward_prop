    "# multiple of 3 and 5\n",
    "\n",
    "def getSum():\n",
    "    sum = 0\n",
    "    for x in range(1, 1000):\n",
    "        if (x % 3 == 0 or x % 5 == 0):\n",
    "            sum += x\n",
    "            \n",
    "    return(sum)\n",
    "\n",
    "print(getSum())"
